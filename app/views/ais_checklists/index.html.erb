<div class="well">
	<%= form_tag import_ais_checklists_path, multipart: true, role: "form", class: "form-inline" do %>
		<div class="form-group">
			<%= file_field_tag :file %>
		</div>
		<button type="submit" class="btn btn-default" name="commit">Import</button>
	<% end %>
</div>

<div class="panel panel-default panel-primary">
	<div class="panel-heading">
		<h3 class="panel-title">AIS Checklists</h3>
	</div>
	<table class="table table-condensed table-striped table-hover">
		<thead>
		  <tr>
		    <th>PIDM</th>
		    <th>Term</th>
		    <th>Application No.</th>
		    <th>Requirement Code</th>
		    <th>Received Date</th>
		    <th>Item</th>
		    <th>Description</th>
		    <th>CKST Code</th>
		    <th>Mandatory</th>
		  </tr>
		</thead>

		<tbody>
			<% @ais_checklists.each do |ais_checklist| %>
			  <tr>
			    <td><%= ais_checklist.banner_pidm %></td>
			    <td><%= ais_checklist.application_term %></td>
			    <td><%= ais_checklist.application_number %></td>
			    <td><%= ais_checklist.requirement_code %></td>
			    <td><%= ais_checklist.received_date %></td>
			    <td><%= ais_checklist.item %></td>
			    <td><%= ais_checklist.item_description %></td>
			    <td><%= ais_checklist.ckst_code %></td>
			    <td><%= ais_checklist.mandatory %></td>
			  </tr>
			<% end %>
		</tbody>
	</table>
	<div class="panel-footer">
		<%= paginate @ais_checklists %>
	</div>
</div>
