<%= bootstrap_form_for @ais_checklist do |f| %>
  
	<%= f.error_messages %>

	<%= f.bs_text_field :banner_pidm, label: "PIDM", placeholder: "PIDM", help_text: "Required." %>
	
	<%= f.bs_text_field :application_term, label: "Term", placeholder: "Term", help_text: "Required." %>
	
	<%= f.bs_text_field :application_number, label: "Application No.", placeholder: "Application No.", help_text: "Required." %>

	<%= f.bs_text_field :requirement_code, label: "Requirement Code", placeholder: "Requirement Code", help_text: "Required." %>

	<%= f.bs_text_field :received_date, label: "Received Date", placeholder: "Received Date" %>

	<%= f.bs_text_field :item, placeholder: "Received Date" %>

	<%= f.bs_text_field :item_description, label: "Item Description", placeholder: "Item Description" %>

	<%= f.bs_text_field :ckst_code, label: "CKST Code", placeholder: "CKST Code" %>

	<%= f.bs_text_field :mandatory, placeholder: "Mandatory" %>

  <%= f.bs_submit "Submit" %>
	
<% end %>
