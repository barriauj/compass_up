<div class="page-header">
  <h2>Import AIS Application Comments</h2>
</div>

<%= render 'list_navigation' %>

<div class="panel panel-default panel-danger">
  <div class="panel-heading">
    <h3 class="panel-title">Warning! This will delete all records before importing.</h3>
  </div>
  <div class="panel-body">
		<%= form_tag import_ais_application_comments_path, multipart: true, role: "form", class: "form-inline" do %>
			<div class="form-group">
				<%= file_field_tag :file %>
			</div>
			<button type="submit" class="btn btn-default btn-danger" name="commit">Import</button>
		<% end %>
	</div>
</div>
